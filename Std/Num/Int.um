module :
  type Int

  extern (+) : Int -> Int -> Int = @umber_primitive(Int.add)
  extern (*) : Int -> Int -> Int = @umber_primitive(Int.mul)

type Int = @umber_primitive(Int)

# Let's not overcomplicate:
  `@umber_primitive(Int.add)` can just be `"@Int.add"`
  `umber_primitive(Int)` can probably just be `Int` and be pre-existing