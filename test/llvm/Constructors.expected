; ModuleID = 'examples/Constructors.um'
source_filename = "examples/Constructors.um"
target datalayout = "i32:64-i64:64-p:64:64-f64:64"

%umber_block = type { %umber_header, [0 x i64] }
%umber_header = type { i16, i16, i32 }

@Constructors.A.Foo = constant %umber_block* inttoptr (i64 1 to %umber_block*)
@Constructors.B.Foo = constant %umber_block* inttoptr (i64 1 to %umber_block*)

define i32 @"umber_main:examples/Constructors.um"() {
entry:
  ret i32 0
}

define tailcc %umber_block* @Constructors.Wrap.Wrap(%umber_block* %"Constructors.Wrap.*arg0.1") {
entry:
  %box = call i64* @umber_gc_alloc(i64 16)
  %box1 = bitcast i64* %box to i16*
  store i16 0, i16* %box1, align 2
  %box2 = getelementptr i16, i16* %box1, i32 1
  store i16 1, i16* %box2, align 2
  %box3 = bitcast i64* %box to %umber_block**
  %box4 = getelementptr %umber_block*, %umber_block** %box3, i32 1
  store %umber_block* %"Constructors.Wrap.*arg0.1", %umber_block** %box4, align 8
  %box5 = bitcast %umber_block** %box3 to %umber_block*
  ret %umber_block* %box5
}

define tailcc %umber_block* @Constructors.B.of_a(%umber_block* %Constructors.B.match.1) {
entry:
  %Constructors.B.Foo = load %umber_block*, %umber_block** @Constructors.B.Foo, align 8
  ret %umber_block* %Constructors.B.Foo
}

define tailcc %umber_block* @Constructors.Wrap.my_id(%umber_block* %Constructors.Wrap.x.1) {
entry:
  %block_field_gep = getelementptr %umber_block, %umber_block* %Constructors.Wrap.x.1, i32 0, i32 1, i32 0
  %block_field_raw = load i64, i64* %block_field_gep, align 8
  %block_field = inttoptr i64 %block_field_raw to %umber_block*
  %box = call i64* @umber_gc_alloc(i64 16)
  %box1 = bitcast i64* %box to i16*
  store i16 0, i16* %box1, align 2
  %box2 = getelementptr i16, i16* %box1, i32 1
  store i16 1, i16* %box2, align 2
  %box3 = bitcast i64* %box to %umber_block**
  %box4 = getelementptr %umber_block*, %umber_block** %box3, i32 1
  store %umber_block* %block_field, %umber_block** %box4, align 8
  %box5 = bitcast %umber_block** %box3 to %umber_block*
  ret %umber_block* %box5
}

declare i64* @umber_gc_alloc(i64)

